<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC

  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.metasocio.model.friendsmanagement.Friends" table="friends">

  <id column="Id" name="id" type="int" >
		<generator class="native"/>
		</id>
		
		<property column="friend_one" name="friend_one" type="int" />
		<property column="friend_two" name="friend_two" type="int" />
		
		<property column="created_at" name="created_at" type="timestamp" />
		<property column="updated_at" name="updated_at" type="timestamp" />
		<property column="created_by" name="created_by" type="string" />
		<property column="updated_by" name="updated_by" type="string" />
		<property column="isdelete" name="isdelete" type="int" />
		
	</class>
	</hibernate-mapping>