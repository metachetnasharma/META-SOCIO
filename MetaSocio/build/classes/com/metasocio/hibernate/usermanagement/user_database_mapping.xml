<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="com.metasocio.model.usermanagement.User" table="user_table">

  <id column="user_id" name="user_id" type="int" >
		<generator class="native"/>
		</id>
		
		<property column="name" name="name" type="string" />
		<property column="email_id" name="email_id" type="string" />
		<property column="image_url" name="image_url" type="string" />
		<property column="role" name="role" type="string" />
		<property column="department" name="department" type="string" />
		<property column="project_name" name="project_name" type="string" />
		<property column="created_at" name="created_at" type="timestamp" />
		<property column="updated_at" name="updated_at" type="timestamp" />
		<property column="created_by" name="created_by" type="string" />
		<property column="updated_by" name="updated_by" type="string" />
		<property column="isdelete" name="isdelete" type="int" />
		<property column="phone_No" name="phone_No" type="string" />
	</class>
	
	
	

</hibernate-mapping>



